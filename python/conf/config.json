{
    "sqlite": {
        "db_path": "/tmp/smiledetector/frames.db"
    },
    "python": {
        "flask_app_name": "smile_detector",
        "logging": {
            "version": 1,
            "formatters": {"default": {
                "format": "[%(asctime)s] %(levelname)s in %(module)s: %(message)s"
            }},
            "handlers": {"wsgi": {
                "class": "logging.StreamHandler",
                "stream": "ext://sys.stderr", 
                "formatter": "default"
            }},
            "root": {
                "level": "DEBUG",
                "handlers": ["wsgi"]
            }
        },
        "network": {
            "hostname": "localhost",
            "port": 5050
        }
    },
    "react": {
        "network": {
            "hostname": "localhost",
            "port": 3000
        }
    }

}